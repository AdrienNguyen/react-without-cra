{
    "name": "webpack-with-reactjs",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "postinstall": "rm -rf .husky && husky install && npx husky add .husky/pre-commit \"npx lint-staged\"",
        "test": "jest --config src/jest.config.js --updateSnapshot",
        "dev": "webpack serve --config webpack/webpack.dev.js --hot",
        "build": "webpack --config webpack/webpack.prod.js",
        "start": "node server.js",
        "start-pm2": "pm2 start ./pm2/prod.json",
        "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
        "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|scss|md)\"",
        "lint": "eslint ."
    },
    "lint-staged": {
        "*.+(js|jsx|ts|tsx)": "eslint --max-warnings 0 --fix",
        "*.+(js|jsx|ts|tsx|json|css|md)": "prettier --write"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.16.5",
        "@babel/preset-env": "^7.16.5",
        "@babel/preset-react": "^7.16.5",
        "@svgr/webpack": "^6.1.2",
        "@testing-library/react": "^12.1.2",
        "@types/jest": "^27.0.3",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-test-renderer": "^17.0.1",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "babel-jest": "^27.4.5",
        "babel-loader": "^8.2.3",
        "copy-webpack-plugin": "^10.1.0",
        "css-loader": "^6.5.1",
        "css-minimizer-webpack-plugin": "^3.2.0",
        "dotenv": "^10.0.0",
        "dotenv-expand": "^5.1.0",
        "eslint": "^7.32.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "express": "^4.17.1",
        "file-loader": "^6.2.0",
        "html-loader": "^3.0.1",
        "html-minimizer-webpack-plugin": "^3.5.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^7.0.4",
        "image-minimizer-webpack-plugin": "^3.1.0",
        "imagemin": "^8.0.1",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-pngquant": "^9.0.2",
        "imagemin-svgo": "^10.0.0",
