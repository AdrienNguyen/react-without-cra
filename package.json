        "sass-loader": "^12.4.0",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.2.5",
        "ts-jest": "^27.1.2",
        "ts-loader": "^9.2.6",
        "url-loader": "^4.1.1",
        "webpack": "^5.65.0",
